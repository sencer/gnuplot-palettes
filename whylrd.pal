# line styles
set linetype 1 lc rgb '#ffffff' # white
set linetype 2 lc rgb '#ffee00' # 
set linetype 3 lc rgb '#ff7000' # yellow
set linetype 4 lc rgb '#ee0000' #
set linetype 5 lc rgb '#7f0000' # red

# palette
set palette defined ( \
    0 '#ffffff', \
    1 '#ffee00', \
    2 '#ff7000', \
    3 '#ee0000', \
    4 '#7f0000')
